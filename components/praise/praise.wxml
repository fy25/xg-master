<view catchtap="praiseTap">
  <view class="praise" hidden="{{isLike=='1'}}">
    <image src="/images/praise.png" />
    <text>{{likeCount}}</text>
  </view>
  <view class="praise praise_cur" hidden="{{isLike=='0'}}">
    <image src="/images/praise_cur.png" />
    <text>{{likeCount}}</text>
  </view>
</view>